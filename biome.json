{
  "$schema": "https://biomejs.dev/schemas/1.6.1/schema.json",
  "linter": {
    "rules": {
      "all": true,
      "style": {
        "noDefaultExport": "off"
      },
      "complexity": {
        "useSimplifiedLogicExpression": "off"
      },
      "nursery": {
        "all": true,
        "noUndeclaredDependencies": "off",
        "noNodejsModules": "off"
      }
    }
  }
}
