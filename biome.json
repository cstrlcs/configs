{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"linter": {
		"rules": {
			"recommended": true,
			"correctness": {
				"noUnusedImports": "off",
				"noUnusedVariables": "off"
			},
			"complexity": {
				"useLiteralKeys": "off"
			},
			"style": {
				"noDefaultExport": "off",
				"useNamingConvention": "off"
			}
		}
	}
}
