{
	"$schema": "https://biomejs.dev/schemas/1.6.4/schema.json",
	"linter": {
		"rules": {
			"all": true,
			"correctness": {
				"noUnusedImports": "off",
				"noUnusedVariables": "off"
			},
			"complexity": {
				"useLiteralKeys": "off"
			},
			"style": {
				"noDefaultExport": "off",
				"useFilenamingConvention": "off",
				"useNamingConvention": "off"
			},
			"nursery": {
				"all": true,
				"noUndeclaredDependencies": "off"
			}
		}
	}
}
